#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
// --------------------------------------------------------------------------------------------------------------------------------------------------------------------
// |   ESC   |    Q    |    W    |    E    |    R    |    T    |                                          |    Y    |    U    |    I    |    O    |    P    |    \    |
// |   TAB   |    A    |    S    |    D    |    F    |    G    |                                          |    H    |    J    |    K    |    L    |    ;    |    '    |
// |  SHIFT  |    Z    |    X    |    C    |    V    |    B    | CTRL+A  | CTRL+C  |  |  CTRL+V |  CTRL+X |    N    |    M    |    ,    |    .    |    /    |  R CTRL |
//                               |   GUI   |   DEL   | RETURN  |  SPACE  | ESCAPE  |  |  RETURN |  SPACE  |   TAB   |   BSPC  |  R ALT  |
            bindings = <
    &bt BT_CLR &bootloader &kp C &kp D     &kp E    &kp F                            &kp F7     &kp F8    &kp F9    &kp F10   &bootloader &bt BT_CLR
    &kp A    &kp SEMI &kp COMMA &kp A &kp P     &kp Y                             &kp F      &kp G     &kp C     &kp R     &kp L    &kp BSLH
    &kp TAB    &kp A    &kp O     &kp E      &kp U     &kp I                             &kp D      &kp H     &kp T     &kp N     &kp S    &kp SQT
    &kp A &kp SQT  &kp Q     &kp J      &kp K     &kp X      &kp A   &kp A      &kp B      &kp M     &kp W     &kp V     &kp C    &kp DOT 
                                             &kp A   &kp A    &kp A &kp A      &kp A    &kp A 
            >;

        };
    };
};
